{
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["es2018"],
    "module": "commonjs",
    "target": "es2018",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "strict": true
  }
}